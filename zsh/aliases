alias ll='ls -lah'

alias ga='git add'
alias gc='git commit'
alias gp='git push'

alias nv='nvim'

alias tmuxa='tmux attach-session -d -t'
alias tmuxl='tmux list-sessions'
alias tmuxks='tmux kill-session'
alias tmuxka-'tmux kill-server'

alias d='dirs -v'
for index ({1..9}) alias "$index"="cd +${index}"; unset index


